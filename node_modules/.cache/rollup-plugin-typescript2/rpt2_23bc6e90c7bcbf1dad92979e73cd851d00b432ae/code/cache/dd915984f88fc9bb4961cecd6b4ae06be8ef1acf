ition}\n\n  :hover{\n    border-color: #1EA7FD50;\n    box-shadow: rgba(0, 0, 0, 0.06) 0 3px 10px 0;\n    transform: translate3d(0, 4px, 0);\n  }\n`;\r\nconst css_hover_left = css `\n  ${css_transition}\n  :hover{\n    border-color: #1EA7FD50;\n    box-shadow: rgba(0, 0, 0, 0.06) 0 3px 10px 0;\n    transform: translate3d(-4px, 0, 0);\n  }\n`;\r\nconst css_hover_right = css `\n  ${css_transition}\n  :hover{\n    border-color: #1EA7FD50;\n    box-shadow: rgba(0, 0, 0, 0.06) 0 3px 10px 0;\n    transform: translate3d(4px, 0, 0);\n  }\n`;\r\nconst css_hover_zoom = css `\n  ${css_transition}\n  :hover{\n    z-index:10;\n    border-color: #1EA7FD50;\n    box-shadow: rgba(0, 0, 0, 0.06) 0 3px 10px 0;\n    transform: scale(1.1);\n  }\n`;\r\n//# sourceMappingURL=styles.js.map","references":["/home/djt/Myproject/react-card-component/node_modules/@emotion/react/types/index.d.ts","/home/djt/Myproject/react-card-component/src/components/Card.tsx","/home/djt/Myproject/react-card-component/src/components/utils.tsx"],"map":"{\"version\":3,\"file\":\"styles.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../../src/components/styles.tsx\"],\"names\":[],\"mappings\":\"AAAA,OAAO,EAAE,GAAG,EAAE,MAAM,gBAAgB,CAAA;AAEpC,OAAO,EAAC,gBAAgB,EAAC,MAAM,SAAS,CAAA;AAExC,MAAM,CAAC,MAAM,QAAQ,GAAG,GAAG,CAAA;;;;;CAK1B,CAAA;AAED,MAAM,CAAC,MAAM,SAAS,GAAG,GAAG,CAAA;;;;;CAK3B,CAAA;AAED,MAAM,CAAC,MAAM,UAAU,GAAG,CAAC,MAAwB,EAAE,EAAE,CAAC,MAAM,IAAI,GAAG,CAAA,sBAAsB,CAAA;AAE3F,MAAM,CAAC,MAAM,WAAW,GAAG,CAAC,MAAwB,EAAE,EAAE,CAAC,MAAM,IAAI,GAAG,CAAA,8BAA8B,CAAA;AAEpG,MAAM,CAAC,MAAM,WAAW,GAAG,CAAC,MAAwB,EAAE,EAAE,CAAC,MAAM,IAAI,GAAG,CAAA,iBAAiB,CAAA;AAEvF,MAAM,CAAC,MAAM,cAAc,GAAG,CAAC,KAAsB,EAAE,EAAE,CAAC,GAAG,CAAA,cAAc,KAAK,IAAE,SAAS,GAAG,CAAA;AAE9F,MAAM,CAAC,MAAM,cAAc,GAAG,CAAC,KAAsB,EAAE,EAAE,CAAC,GAAG,CAAA,GAAG,gBAAgB,CAAC,KAAK,CAAC,EAAE,CAAA;AAEzF,MAAM,CAAC,MAAM,gBAAgB,GAAE,CAAC,IAAqB,EAAC,EAAE;IACtD,QAAQ,IAAI,EAAE;QACZ,KAAK,IAAI;YACP,OAAO,YAAY,CAAC;QACtB,KAAK,MAAM;YACT,OAAO,cAAc,CAAC;QACxB,KAAK,MAAM;YACT